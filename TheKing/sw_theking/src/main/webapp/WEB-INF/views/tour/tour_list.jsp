<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Tour List</title>
<style>
#svg-서울 svg {
    width: 70%;     /* 혹은 500px 등으로 지정 가능 */
    height: auto;
    max-width: 700px;
}
</style>
 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
 <script>
    //핸들러 연결
    $(document).ready(function() {
    // 전체 SVG 보이기 / 숨기기
    $("#wideBtn").click(function() {
        $(".region").hide();  // 모든 지역 SVG 숨기기
        $("#svgMap").toggle(); // 전체 SVG 맵의 표시/숨김 전환
    });

    // 지역 버튼 클릭 시
    $(".regionBtn").click(function() {
        var region = $(this).data("region");  // 클릭한 버튼의 지역 이름 가져오기
        
        $(".region").hide();  // 모든 지역 SVG 숨기기
        $("#svgMap").show();  // 전체 맵 보이기
        $("#svg-" + region).show();  // 클릭한 지역만 보이게 하기
    });
});

 </script>
